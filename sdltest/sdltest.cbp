<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="sdltest" />
		<Option pch_mode="2" />
		<Option compiler="gcc-mingw32" />
		<Build>
			<Target title="Debug">
				<Option output="bin/debug/sdltest" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin/obj/debug/" />
				<Option type="1" />
				<Option compiler="gcc-mingw32" />
			</Target>
			<Target title="Release">
				<Option output="bin/release/sdltest" prefix_auto="1" extension_auto="1" />
				<Option object_output="bin/obj/release/" />
				<Option type="0" />
				<Option compiler="gcc-mingw32" />
			</Target>
		</Build>
		<Compiler>
			<Add directory="$(#sdl2.include)/SDL2" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="SDL2main" />
			<Add library="SDL2.dll" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add directory="$(#sdl2.lib)" />
		</Linker>
		<ExtraCommands>
			<Add after="XCOPY $(#sdl2)\bin\*.dll $(TARGET_OUTPUT_DIR) /D /Y" />
		</ExtraCommands>
		<Unit filename="main.cpp" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
